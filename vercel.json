{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "vite",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://ltd-leland-adkhex-a2e557e5.koyeb.app/api/:path*"
    },
    {
      "source": "/uploads/:path*",
      "destination": "https://ltd-leland-adkhex-a2e557e5.koyeb.app/uploads/:path*"
    },
    {
      "source": "/assets/:path*",
      "destination": "/assets/:path*"
    },
    {
      "source": "/favicon.ico",
      "destination": "/favicon.ico"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
